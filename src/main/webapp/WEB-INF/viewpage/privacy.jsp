	<script>
	 $(document).ready(function(){ 
         $(window).scroll(function(){ 
             if ($(this).scrollTop() > 50) { 
                 $('#scroll').fadeIn(); 
                 $('.header').addClass('fix-header');
             } else { 
                 $('#scroll').fadeOut(); 
                 $('.header').removeClass('fix-header');
             } 
         }); 
         $('#scroll').click(function(){ 
             $("html, body").animate({ scrollTop: 0 }, 600); 
             return false; 
         }); 
     });
	</script>
	
